<?xml version="1.0" encoding="utf-8"?>
<shape
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <!-- 테두리 내부 색상 설정 -->
    <solid android:color="#00FFFFFF" />

    <!-- 테두리 둥근 모양 설정 -->
    <corners android:radius="0dp" />

    <!-- 테두리 내부 여백 설정 -->
    <padding
        android:bottom="5dp"
        android:left="5dp"
        android:right="5dp"
        android:top="5dp" />

    <!-- 테두리 설정: 두께, 색상 -->
    <stroke
        android:width="0.5dp"
        android:color="#b8babc" />

</shape>
